# Skill Agent Tree Environment Configuration Example
# Copy this file to .env and fill in your actual values

# DashScope Configuration
DASHSCOPE_API_KEY=sk-your-api-key-here
DASHSCOPE_MODEL=qwen-max

# Alternative: Alibaba Cloud DashScope
# DASHSCOPE_API_KEY=your-dashscope-key
# DASHSCOPE_MODEL=qwen-max

# Alternative: Anthropic Claude
# ANTHROPIC_API_KEY=your-anthropic-key
# ANTHROPIC_MODEL=claude-3-sonnet-20240229

# AgentScope Configuration
AGENTSCOPE_LOG_LEVEL=INFO
AGENTSCOPE_PROJECT_NAME=SkillAgentTree

# System Configuration
MAX_AGENT_ITERATIONS=10
ENABLE_STREAMING=True
ENABLE_PARALLEL_TOOLS=False

# Optional: Studio Integration
# STUDIO_URL=http://localhost:8000
# TRACING_URL=http://localhost:8000/tracing
